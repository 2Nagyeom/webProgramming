from \"../../store\";\n\nconst nearCenterArr = [\n  \"고용\",\n  \"관련유관단체\",\n  \"권익\",\n  \"보육/교육\",\n  \"복지관\",\n  \"여가/문화\",\n  \"의료/건강\",\n  \"이용시설\",\n  \"장애인단체\",\n];\n\nexport default (container) => {\n  let template = `\n    <div>\n      <h1>메인페이지</h1>\n        ${inputWithTitle(\"selectBoxToCompany\", \"직장선택\")}\n        ${inputWithTitle(\"selectBoxToCenter\", \"선호 시설 선택\")}\n      <div id='mainPageResultBtn'>\n        <h3>결과보기</h3>\n      </div>\n      <div id='likePlaceSelectModal' style=\"position:absolute;top:20vh;left:10%;width:80%;height:300px;background-color:lightgray;display:none\";border-radius: 10% 10% 50% 50%;>\n        <div style=\"display:flex;justify-content:space-between;padding:3% 5% 3% 5%\">\n          <span>선호시설 선택</span>\n          <div id='likePlaceSelectModalCancel'>x</div>\n        </div>\n        <div style=\"height:100%;overflow-y:auto\">\n          <div style=\"display:flex;flex-direction:column;\">\n            ${nearCenterArr\n              .map((v, i) => selectBox(\"selectBox_\" + i, v))\n              .join(\"\")}\n          </div>\n        </div>\n      </div>\n    </div>\n  `;\n\n  container.innerHTML = template;\n\n  const { center } = store.getState();\n\n  store.subScrip(() => {\n    const getStateCenter = store.getState().center;\n    const selectBoxToCenter = document.getElementById(\"selectBoxToCenter\");\n    selectBoxToCenter.innerText = getStateCenter.join(\", \");\n  });\n\n  return null;\n};\n\nconst selectBox = (id, text) => {\n  return `\n    <div id=${id} style=\"width:90%;height:100%;background-color:white;padding:3% 5% 3% 5%\";>\n      <span>${text}</span>\n    </div>\n  `;\n};\n\nconst inputWithTitle = (id, title) => {\n  return `\n        <div style=\"display:flex;flex-direction:row;\">\n            <span>${title}</span>\n            <div id=${id} style=\"width:300px;height:30px;border-width:thin;background-color:yellow;padding:3%;\"></div>\n        </div>\n    `;\n};\n"},"lineCount":null}},"error":null,"hash":